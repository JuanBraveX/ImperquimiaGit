<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PrecioTotal__c</fullName>
    <description>Precio total del producto de pedido y el descuento aplicado</description>
    <externalId>false</externalId>
    <formula>((1- DescuentoSAP__c )*(UnitPrice * Quantity )  * (1-IMP_DescuentoPromo__c))   *(1- DescuentoManual__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Precio total</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
